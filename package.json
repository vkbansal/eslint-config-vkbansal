{
  "name": "eslint-config-vkbansal",
  "version": "4.5.1",
  "description": "Shareable eslint config",
  "main": "index.js",
  "scripts": {
    "check": "node check.js",
    "check:react": "node check.js react",
    "check:import": "node check.js import",
    "check:all": "node check.js && node check.js react && node check.js import",
    "test": "node index.js && node react.js && node import.js && node standard.js && node jsx-control-statements",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkbansal/eslint-config-vkbansal.git"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vkbansal/eslint-config-vkbansal/issues"
  },
  "homepage": "https://github.com/vkbansal/eslint-config-vkbansal#readme",
  "engines": {
    "node": ">=5.0.0"
  },
  "peerDependencies": {
    "eslint": "~3.17.0",
    "eslint-import-resolver-webpack": "~0.8.1",
    "eslint-plugin-import": "~2.2.0"
  },
  "optionalDependencies": {
    "eslint-plugin-jsx-control-statements": "~2.1.1",
    "eslint-plugin-react": "~6.10.0"
  },
  "devDependencies": {
    "axios": "~0.15.3",
    "chalk": "~1.1.3",
    "cheerio": "~0.22.0",
    "eslint": "~3.17.0",
    "eslint-config-airbnb": "~14.1.0",
    "eslint-config-airbnb-base": "~11.1.1",
    "eslint-config-prettier": "~1.5.0",
    "eslint-import-resolver-webpack": "~0.8.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~4.0.0",
    "eslint-plugin-jsx-control-statements": "~2.1.1",
    "eslint-plugin-react": "~6.10.0",
    "eslint-plugin-standard": "~2.1.1",
    "husky": "^0.13.2",
    "json-diff": "^0.3.1",
    "lint-staged": "^3.4.0",
    "prettier": "^0.22.0"
  },
  "lint-staged": {
      "*.js": [
          "prettier --single-quote --tab-width 4 --print-width 120 --trailing-comma none --jsx-bracket-same-line --write",
          "git add"
      ]
  }
}

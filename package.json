{
    "name": "eslint-config-vkbansal",
    "version": "5.2.1",
    "description": "Shareable eslint config",
    "main": "index.js",
    "files": [
        "index.js",
        "import.js",
        "react.js",
        "typescript.js",
        "core",
        "react",
        "import"
    ],
    "scripts": {
        "compare": "node scripts/check.js",
        "compare:react": "node scripts/check.js react",
        "compare:import": "node scripts/check.js import",
        "compare:all": "yarn compare && yarn compare:react && yarn compare:import",
        "fmt:check": "prettier --check index.js import.js react.js typescript.js core/*.js react/*.js import/*.js",
        "fmt": "prettier --write index.js import.js react.js typescript.js core/*.js react/*.js import/*.js",
        "test": "yarn fmt:check node index.js && node react.js && node import.js",
        "precommit": "lint-staged",
        "prepublishOnly": "yarn test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vkbansal/eslint-config-vkbansal.git"
    },
    "keywords": [
        "eslint-config"
    ],
    "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vkbansal/eslint-config-vkbansal/issues"
    },
    "homepage": "https://github.com/vkbansal/eslint-config-vkbansal#readme",
    "engines": {
        "node": ">=8.9.0"
    },
    "peerDependencies": {
        "eslint": "^5.16.0",
        "eslint-plugin-import": "^2.17.3",
        "@typescript-eslint/parser": "^1.10.2",
        "@typescript-eslint/eslint-plugin": "^1.10.2"
    },
    "optionalDependencies": {
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^1.10.2",
        "@typescript-eslint/eslint-plugin": "^1.10.2",
        "axios": "^0.19.0",
        "chalk": "^2.4.2",
        "cheerio": "^1.0.0-rc.3",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^5.0.0",
        "eslint-import-resolver-webpack": "^0.11.1",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-jsx-control-statements": "^2.2.1",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-standard": "^4.0.0",
        "husky": "^2.4.1",
        "json-diff": "^0.5.4",
        "lint-staged": "^8.2.1",
        "prettier": "^1.18.2"
    },
    "lint-staged": {
        "*.js": [
            "prettier --single-quote --tab-width 4 --print-width 120 --trailing-comma none --jsx-bracket-same-line --write",
            "git add"
        ]
    }
}
